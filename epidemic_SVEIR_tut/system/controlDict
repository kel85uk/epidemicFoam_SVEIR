FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     epidemicFOAM_SVEIR;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1000;

deltaT          0.02;

writeControl    adjustableRunTime;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

adjustTimeStep no;

maxCo 0.5;

runTimeModifiable yes;

